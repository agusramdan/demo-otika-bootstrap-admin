<?xml version="1.0"?>
<thlogic>
    <attr sel="#domainData"
          method="post"
          role="form"
          th:object="${domain}"
          th:action="@{/idm/users/save}" >
        <attr sel="#username" th:field="*{id}" />
        <attr sel="#firstName" th:field="*{firstName}" />
        <attr sel="#lastName" th:field="*{lastName}" />
        <attr sel="#enabled" th:field="*{enabled}" />
    </attr>
</thlogic>